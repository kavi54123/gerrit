{
    "name": "gerritstats",
    "version": "1.0.0",
    "description": "GerritStats - Gerrit Code Review statistics analysis tool",
    "main": "index.html",
    "author": "Lasse Holmstedt",
    "license": "MIT",
    "private": true,
    "dependencies": {
        "bootstrap": "^3.3.7",
        "classnames": "^2.2.5",
        "css-element-queries": "^0.3.2",
        "d3": "<4.x.x",
        "d3-svg-legend": "^1.12.0",
        "immutable": "^3.8.1",
        "moment": "^2.14.1",
        "node-sass": "^4.13.1",
        "npm": "^3.10.5",
        "numeral": "^1.5.3",
        "react": "^15.3.1",
        "react-bootstrap": "^0.30.3",
        "react-dom": "^15.3.1",
        "react-router": "^2.7.0",
        "reactable": "^0.14.0"
    },
    "scripts": {
        "cleanJava": "../gradlew clean",
        "buildJava": "../gradlew build",
        "testJava": "../gradlew test",
        "generateData": "rm -rf out-html/data && java -Xmx4096m -Xms256m -jar build/libs/GerritStats.jar -o out-html/data",
        "webpack": "webpack --config webpack.config.js",
        "webpack-watch": "webpack --watch --config webpack.config.js",
        "webpack:project": "webpack --config webpack.config.projects.js",
        "prewebpack": "mkdir -p out-html && cp src/main/frontend/index.html out-html/",
        "prewebpack-watch": "mkdir -p out-html && cp src/main/frontend/index.html out-html/",
        "prewebpack:project": "mkdir -p out-html-project && cp src/main/frontend/index-project.html out-html-project/",
        "init-project-data": "mkdir -p out-html-project/data && cp ../../gerritstats-extension/project-level-stats.js out-html-project/data",
        "eslint": "eslint --ext .js,.jsx src/main/frontend/",
        "start": "webpack serve --config webpack.config.js --port 9000"
    },
    "repository": {
        "type": "git",
        "url": "git@github.com:holmari/gerritstats.git"
    },
    "devDependencies": {
        "@babel/core": "^7.25.2",
        "@babel/preset-env": "^7.25.4",
        "@babel/preset-react": "^7.24.7",
        "babel-cli": "^6.14.0",
        "babel-core": "^6.14.0",
        "babel-loader": "^8.3.0",
        "babel-preset-es2015": "^6.14.0",
        "babel-preset-react": "^6.11.1",
        "css-loader": "^5.2.7",
        "eslint": "^7.32.0",
        "eslint-plugin-react": "^7.30.1",
        "file-loader": "^6.2.0",
        "html-loader": "^5.1.0",
        "react-hot-loader": "^4.13.1",
        "sass-loader": "^12.6.0",
        "style-loader": "^2.0.0",
        "url-loader": "^4.1.1",
        "webpack": "^5.94.0",
        "webpack-cli": "^5.1.4",
        "webpack-dev-server": "^5.0.4"
    }
}
